{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "nqasm",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#functions"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.nqasm",
				"match": "\\b(if|while|for|return)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.nqasm",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.nqasm",
					"match": "\\\\."
				}
			]
		},
		"functions": {
			"patterns": [
				{
					"name": "entity.function.nqasm",
					"match": "^([a-zA-Z_][a-zA-Z0-9_]*:)"
				}
			]
		}
	},
	"scopeName": "source.nqasm"
}